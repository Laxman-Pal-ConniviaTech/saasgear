{
  "name": "api",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register server.ts",
    "dev": "nodemon --experimental-specifier-resolution=node server",
    "db:create-products": "node --es-module-specifier-resolution=node scripts/create-products.js",
    "db:migrate": "knex migrate:latest --knexfile config/knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile config/knexfile.ts",
    "db:create": "knex migrate:make --knexfile config/knexfile.ts",
    "db:down": "knex migrate:down --knexfile config/knexfile.ts",
    "db:seed": "knex seed:run --knexfile config/knexfile.ts",
    "postinstall": "link-module-alias && npx npm-force-resolutions",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix"
  },
  "resolutions": {
    "**/**/fs-capacitor": "^6.2.0",
    "**/graphql-upload": "^11.0.0"
  },
  "dependencies": {
    "@sentry/node": "^7.54.0",
    "apollo-server-express": "^3.0.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bunyan": "^1.8.15",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.8",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "fastest-validator": "^1.17.0",
    "graphql": "^16.6.0",
    "graphql-resolvers": "^0.4.2",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "link-module-alias": "^1.2.0",
    "lodash": "^4.17.21",
    "mailgun-js": "^0.22.0",
    "mjml": "^4.14.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.3.4",
    "redis": "^4.6.7",
    "stripe": "^12.9.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.8",
    "@types/cookie-parser": "^1.4.3",
    "@types/eslint": "^8.40.1",
    "@types/express": "^4.17.17",
    "@types/graphql-resolvers": "^0.3.1",
    "@types/graphql-upload": "^16.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/mailgun-js": "^0.22.13",
    "@types/mjml": "^4.7.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "tsconfig-paths": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "_moduleAliases": {
    "~": "."
  }
}
